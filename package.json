{
  "name": "rust-loader",
  "version": "1.1.1",
  "description": "Webpack loader for Rust",
  "main": "lib.js",
  "repository": "https://github.com/Ralvke/rust-loader",
  "author": "Maxim lis <ralvke@gmail.com>",
  "license": "MIT",
  "scripts": {
    "watch": "node_modules/.bin/babel src/index.js --out-file lib.js --watch",
